<UserControl x:Class="Castor.EdgeLine"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Castor"
             mc:Ignorable="d" 
             d:DesignHeight="0" d:DesignWidth="0">
    <Canvas Background="Transparent" x:Name="Wrapper">
        <Line x:Name="Line1" X1="0" Y1="0" X2="0" Y2="0" Stroke="Black" StrokeThickness="1" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Line x:Name="Line2" X1="0" Y1="0" X2="0" Y2="0" Stroke="Black" StrokeThickness="1" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
        <Line x:Name="Line3" X1="0" Y1="0" X2="0" Y2="0" Stroke="Black" StrokeThickness="1" VerticalAlignment="Bottom" HorizontalAlignment="Left"/>
    </Canvas>
</UserControl>
