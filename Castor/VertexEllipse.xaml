<UserControl x:Class="Castor.VertexEllipse"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Castor"
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="30" VerticalAlignment="Top" HorizontalAlignment="Left">
    <Ellipse x:Name="Ellipse" VerticalAlignment="Top" HorizontalAlignment="Left" Height="30" Width="30" StrokeThickness="2" Stroke="Black">
        <Ellipse.ContextMenu>
            <ContextMenu>
                <MenuItem x:Name="Eigenschaften"/>
                <MenuItem x:Name="Löschen"/>
                <Separator/>
                <MenuItem x:Name="MenuItem1">
                    <MenuItem x:Name="MenuItem2"/>
                    <MenuItem x:Name="MenuItem3"/>
                </MenuItem>
                <MenuItem x:Name="MenuItem4"/>
            </ContextMenu>
        </Ellipse.ContextMenu>
    </Ellipse>
</UserControl>
